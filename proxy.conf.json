{
  "/99minutos/*": {
    "target": "https://sandbox.99minutos.com",
    "secure": false,
    "pathRewrite": {
      "^/99minutos": ""
    },
    "changeOrigin": true
  },
  "/x14.asmx": {
    "target": "http://ws.exel.com.mx:8181",
    "secure": false,
    "changeOrigin": true
  },
  "/catalogo_clientes_xml/*": {
    "target": "http://www.grupocva.com",
    "secure": false,
    "changeOrigin": true
  },
  "/pedidos_web/*": {
    "target": "https://www.grupocva.com",
    "secure": false,
    "changeOrigin": true
  },
  "/api/*": {
    "target": "https://www.grupocva.com",
    "secure": false,
    "changeOrigin": true,
    "headers": {
      "Access-Control-Allow-Origin": "http://localhost:4200",
      "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
      "Access-Control-Allow-Headers": "Content-Type, Authorization",
      "Access-Control-Allow-Credentials": "true"
    }
  }
}
